package SchedulesNTings;
import java.util.Scanner;

public class Employees {
	
	protected String name;
	protected Integer ID;
	protected String availability;
	protected boolean manager;
	Scanner scan = new Scanner(System.in);
	Employees boss = new Employees("Dylan Holt", 000001,"06:30 - 20:00", true);
	
	
	
	public Employees(){
		name = "";
		ID = 000000;
		availability = "";
		manager = false;
	}
	
	public Employees(String n, Integer id, String av, boolean m){
		name = n;
		ID = id;
		availability = av;
		manager = m;
	}
	
	public void setName(String n){
		name = n;
	}
	
	public void setID(Integer i){
		ID = i;
	}
	
	public void setAvailability(String a){
		availability = a;
	}
	
	public void setManager_Status(boolean m){
		manager = m;
	}
	
	public String getName(){
		return name;
	}
	
	public Integer getID(){
		return ID;
	}
	
	public String getAvailability(){
		return availability;
	}
	
	public boolean getManager_Status(){
		return manager;
	}
	
	/*public void Permissions(){
		NEEDS TO BE DONE AT A LATER TIME
	}*/

}
