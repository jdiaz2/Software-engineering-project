package SchedulesNTings;

public class Employees {
	
	protected String name;
	protected Integer ID;
	private String availability;
	protected boolean manager;
	
	
	public Employees(String n, Integer id, String av, boolean m){
		name = n;
		ID = id;
		availability = av;
		manager = m;
	}
	
	public void Set_Name(String n){
		name = n;
	}
	
	public void Set_ID(Integer i){
		ID = i;
	}
	
	public void Set_Availability(String a){
		availability = a;
	}
	
	public void Set_Manager_Status(boolean m){
		manager = m;
	}
	
	public String Get_Name(){
		return name;
	}
	
	public Integer Get_ID(){
		return ID;
	}
	
	public String Get_Availability(){
		return availability;
	}
	
	public boolean Get_Manager_Status(){
		return manager;
	}
	
	/*public void Permissions(){
		NEEDS TO BE DONE AT A LATER TIME
	}*/

}
